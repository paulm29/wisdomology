<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">

    <property name="datetime.current" dbms="postgresql" value="localtimestamp"/>
    <property name="datetime.current" dbms="h2" value="current_timestamp()"/>

    <property name="json.type" dbms="postgresql" value="jsonb"/>
    <property name="json.type" dbms="h2" value="json"/>

    <include file="sql/2023/changelog-2023.xml" relativeToChangelogFile="true"/>

</databaseChangeLog>